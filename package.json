    "body-parser": "^1.17.2",
    "boxen": "^1.2.1",
    "busboy": "^0.2.14",
    "cli-color": "^1.2.0",
    "del": "^3.0.0",
    "es6-promise": "^4.1.1",
    "express": "^4.15.4",
    "express-session": "^1.15.5",
    "express-useragent": "^1.0.7",
    "file-saver": "^2.0.0",
    "file-slicer": "^1.0.0",
    "fs-extra-promise": "^1.0.1",
    "ip": "^1.1.5",
