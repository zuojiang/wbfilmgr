{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "better-npm-run start",
    "test": "better-npm-run test",
    "test:server": "babel-watch -w ./src ./src/server/index.es",
    "test:client": "webpack-dev-server --progress",
    "compile": "npm-run-all clean compile:server compile:client",
    "compile:server": "babel ./src --ignore ./src/client -D --out-dir ./out",
    "compile:client": "webpack --progress -p",
    "clean": "rm -rf ./out/*"
  },
  "betterScripts": {
    "start": {
      "command": "node ./out/server/index.js",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test": {
      "command": "npm-run-all -p test:*",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-css-modules-transform": "^1.2.7",
    "babel-plugin-import": "^1.4.0",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-watch": "^2.0.7",
    "better-npm-run": "^0.1.0",
    "css-loader": "^0.28.5",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "npm-run-all": "^4.0.2",
    "postcss": "^6.0.9",
    "postcss-calc": "^6.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-modules": "^0.8.0",
    "postcss-utilities": "^0.7.0",
    "precss": "^2.0.0",
    "url-loader": "^0.5.9",
